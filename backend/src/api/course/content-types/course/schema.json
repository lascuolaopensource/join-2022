{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "fields": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::field.field",
      "inversedBy": "courses"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "meetings": {
      "displayName": "meetings",
      "type": "component",
      "repeatable": true,
      "component": "time.meetings",
      "required": true
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "enrollmentDeadline": {
      "type": "datetime",
      "required": true
    },
    "enrollmentMin": {
      "type": "integer",
      "required": false
    },
    "enrollmentMax": {
      "type": "integer",
      "required": false
    },
    "preconditionsNeeded": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "cvNeeded": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "portfolioNeeded": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "motivationalLetterNeeded": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "course"
    },
    "confirmed": {
      "type": "boolean",
      "required": true
    },
    "roles": {
      "type": "component",
      "repeatable": true,
      "component": "people.credits",
      "required": true
    },
    "places": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::place.place",
      "inversedBy": "courses"
    }
  }
}
