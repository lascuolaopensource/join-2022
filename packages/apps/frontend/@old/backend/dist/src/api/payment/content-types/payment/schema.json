{
    "kind": "collectionType",
    "collectionName": "payments",
    "info": {
        "singularName": "payment",
        "pluralName": "payments",
        "displayName": "Payment",
        "description": ""
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "uid": {
            "type": "uid",
            "minLength": 32,
            "required": true
        },
        "confirmationCode": {
            "type": "uid",
            "required": true,
            "minLength": 32,
            "private": true
        },
        "owner": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "payments"
        },
        "enrollment": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::enrollment.enrollment",
            "inversedBy": "payment"
        },
        "executed": {
            "type": "boolean",
            "default": false,
            "required": true
        },
        "deadline": {
            "type": "datetime",
            "required": true
        },
        "billingData": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::billing-data.billing-data",
            "inversedBy": "payment"
        }
    }
}
