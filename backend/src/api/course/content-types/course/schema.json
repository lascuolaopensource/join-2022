{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "default": " "
    },
    "preconditionsNeeded": {
      "type": "boolean",
      "default": false
    },
    "price": {
      "type": "integer"
    },
    "confirmed": {
      "type": "boolean",
      "required": true
    },
    "enrollmentDeadline": {
      "type": "datetime"
    },
    "enrollmentMin": {
      "type": "integer",
      "required": true
    },
    "enrollmentMax": {
      "type": "integer",
      "required": false
    },
    "meetings": {
      "type": "component",
      "repeatable": true,
      "component": "time.meeting"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "teachers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-info.user-info",
      "mappedBy": "coursesHeld"
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "course"
    },
    "cvNeeded": {
      "type": "boolean",
      "default": false
    },
    "portfolioNeeded": {
      "type": "boolean",
      "default": false
    },
    "motivationalLetterNeeded": {
      "type": "boolean",
      "default": false
    },
    "areas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::area.area",
      "inversedBy": "courses"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
